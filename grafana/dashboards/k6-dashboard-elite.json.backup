{
  "title": "K6 Elite Performance War Room",
  "uid": "k6-elite",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "3s",
  "time": { "from": "now-30m", "to": "now" },
  "annotations": { "list": [] },
  "panels": [
    {
      "type": "timeseries",
      "title": "Latency Percentiles (Mean Duration)",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_duration\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          }
        }
      }
    },
    {
      "type": "heatmap",
      "title": "Latency Heatmap",
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_duration\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests Per Second (Traffic)",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 6 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_reqs\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: count, createEmpty: false)",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 6 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_failed\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"rate\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\\n  |> map(fn: (r) => ({ r with _value: r._value * 100.0 }))",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Virtual Users (Load)",
      "gridPos": { "x": 0, "y": 22, "w": 12, "h": 6 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"vus\" or r[\"_measurement\"] == \"vus_max\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\" or r[\"_field\"] == \"gauge\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "E"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Iterations",
      "gridPos": { "x": 12, "y": 22, "w": 12, "h": 6 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"iterations\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: count, createEmpty: false)",
          "refId": "F"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bandwidth In / Out",
      "gridPos": { "x": 0, "y": 28, "w": 24, "h": 6 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"data_received\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: sum, createEmpty: false)",
          "refId": "G"
        },
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"data_sent\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: sum, createEmpty: false)",
          "refId": "H"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Phase Breakdown",
      "gridPos": { "x": 0, "y": 34, "w": 24, "h": 8 },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_connecting\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "I"
        },
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_tls_handshaking\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "J"
        },
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_sending\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "K"
        },
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_waiting\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "L"
        },
        {
          "query": "from(bucket: \"loadtests\")\\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\\n  |> filter(fn: (r) => r[\"_measurement\"] == \"http_req_receiving\")\\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
          "refId": "M"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    }
  ]
}
